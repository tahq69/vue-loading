{
  "name": "crip-vue-loading",
  "description": "Loading bar for Vue.js apps using axios and vue-router",
  "version": "1.2.3",
  "author": "Igors Krasjukovs <tahq69@gmail.com>",
  "private": false,
  "main": "dist/build.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist/build.js",
    "dist/build.js.map",
    "types/*.d.ts"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build-app": "cross-env NODE_ENV=production webpack --config webpack.config --progress --hide-modules",
    "build-lib": "cross-env NODE_ENV=production webpack --config webpack.config.lib --progress --hide-modules",
    "build": "npm run build-app && npm run build-lib",
    "release": "node release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tahq69/vue-loading.git"
  },
  "devDependencies": {
    "@types/node": "^8.5.1",
    "axios": "^0.16.2",
    "crip-vue-notice": "^1.0.6",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.5.3",
    "prettier": "1.9.2",
    "sass-loader": "^6.0.6",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.18.2",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "url-loader": "^0.5.9",
    "vue": "^2.5.11",
    "vue-loader": "^12.2.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
